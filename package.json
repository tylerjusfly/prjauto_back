{
	"name": "autoback",
	"version": "1.0.0",
	"type": "module",
	"description": "backend to project auto in typescript",
	"main": "index.js",
	"scripts": {
		"dev": "nodemon dist/index.js",
		"node:dev": "nodemon --watch src --exec \"tsc && npm run copy-assets && node dist/index.js\"",
		"start": "yarn build && yarn serve",
		"build": "tsc && npm run copy:assets",
		"serve": "node dist/index.js",
		"copy:assets": "cp -r src/mailproviders/templates dist/mailproviders/templates",
		"test": "jest"
	},
	"keywords": [
		"typescript"
	],
	"author": "tylerjusfly",
	"license": "ISC",
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.4.1",
		"express": "^4.18.2",
		"express-handlebars": "^7.1.2",
		"joi": "^17.13.1",
		"jsonwebtoken": "^9.0.2",
		"moment": "^2.30.1",
		"nodemailer": "^6.9.13",
		"nodemailer-express-handlebars": "^6.1.0",
		"pg": "^8.11.3",
		"redis": "^4.6.12",
		"reflect-metadata": "^0.2.2",
		"stripe": "^17.7.0",
		"typeorm": "^0.3.21"
	},
	"devDependencies": {
		"@types/cors": "^2.8.14",
		"@types/express": "^4.17.19",
		"@types/jest": "^29.5.14",
		"@types/jsonwebtoken": "^9.0.3",
		"@types/node": "^20.8.4",
		"@types/nodemailer": "^6.4.14",
		"@types/nodemailer-express-handlebars": "^4.0.5",
		"concurrently": "^9.1.2",
		"jest": "^29.7.0",
		"nodemon": "^3.0.1",
		"ts-jest": "^29.2.6",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=20.11.0"
	}
}
